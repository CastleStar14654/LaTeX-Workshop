{
  "deps": [
    {
      "name": "adjustbox"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "doibanner",
      "arg": {
        "format": "{}",
        "snippet": "doibanner{${1:DOI}}"
      }
    },
    {
      "name": "doibanner",
      "arg": {
        "format": "[]{}",
        "snippet": "doibanner[${2:options}]{${1:DOI}}"
      }
    },
    {
      "name": "doibanner*",
      "arg": {
        "format": "{}",
        "snippet": "doibanner*{${1:DOI}}"
      }
    },
    {
      "name": "doibanner*",
      "arg": {
        "format": "[]{}",
        "snippet": "doibanner*[${2:options}]{${1:DOI}}"
      }
    },
    {
      "name": "doiurl",
      "arg": {
        "format": "{}",
        "snippet": "doiurl{${1:DOI}}"
      }
    },
    {
      "name": "doiurl*",
      "arg": {
        "format": "{}",
        "snippet": "doiurl*{${1:DOI}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/doibanner#c": [
      "label=${1:string}",
      "urlbase=${1:string}",
      "width="
    ]
  },
  "args": [
    "\\usepackage/doibanner#c"
  ]
}