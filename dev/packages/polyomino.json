{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "polyomino",
      "arg": {
        "format": "{}",
        "snippet": "polyomino{${1:polyomino spec}}"
      }
    },
    {
      "name": "polyomino",
      "arg": {
        "format": "[]{}",
        "snippet": "polyomino[${2:options%keyvals}]{${1:polyomino spec}}",
        "keys": [
          "\\polyomino#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\polyomino#c": [
      "at=${1:point}",
      "empty cell=${1:token list}",
      "grid",
      "grid style={${1:options}}",
      "p={${1:name}}{${2:options}}",
      "row sep=${1:token list}"
    ]
  },
  "args": []
}