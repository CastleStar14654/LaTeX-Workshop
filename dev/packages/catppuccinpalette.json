{
  "deps": [
    {
      "name": "pgfopts"
    },
    {
      "name": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "CtpReinit"
    },
    {
      "name": "CtpReinit",
      "arg": {
        "format": "[]",
        "snippet": "CtpReinit[${1:options%keyvals}]",
        "keys": [
          "\\usepackage/catppuccinpalette#c,\\CtpReinit#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/catppuccinpalette#c,\\CtpReinit#c": [
      "style=${1|latte,frappe,macchiato,mocha|}",
      "latte",
      "frappe",
      "macchiato",
      "mocha",
      "textcolor",
      "pagecolor",
      "styleAll"
    ]
  },
  "args": [
    "\\usepackage/catppuccinpalette#c,\\CtpReinit#c"
  ]
}